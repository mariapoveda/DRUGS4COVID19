    <!-- https://w3id.org/def/DRUGS4COVID19#causedByDrug -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#causedByDrug">
        <owl:inverseOf rdf:resource="https://w3id.org/def/DRUGS4COVID19#hasEffect"/>
        <rdfs:comment xml:lang="en">A relation between an effect and a drug that causes it</rdfs:comment>
        <rdfs:label xml:lang="es">Causado por medicamento</rdfs:label>
        <rdfs:label xml:lang="en">Caused by drug</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#contains -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#contains">
        <rdfs:comment xml:lang="en">A relatiion between a whole and one of its parts</rdfs:comment>
        <rdfs:label xml:lang="en">Contains</rdfs:label>
        <rdfs:label xml:lang="es">Contiene</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#esTratadaConMedicamento -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#esTratadaConMedicamento">
        <owl:inverseOf rdf:resource="https://w3id.org/def/DRUGS4COVID19#trataEnfermedad"/>
        <rdfs:label xml:lang="es">es tratada con medicamento</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#hasActiveSubstance -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#hasActiveSubstance">
        <owl:inverseOf rdf:resource="https://w3id.org/def/DRUGS4COVID19#isActiveSubstanceOfDrug"/>
        <rdfs:comment xml:lang="en">A relation between a drug and its active substance</rdfs:comment>
        <rdfs:label xml:lang="en">Has active substance</rdfs:label>
        <rdfs:label xml:lang="es">Tiene principio activo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#hasEffect -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#hasEffect">
        <rdfs:comment xml:lang="en">A relation between a drug and an effect it causes</rdfs:comment>
        <rdfs:label xml:lang="en">Has effect</rdfs:label>
        <rdfs:label xml:lang="es">Tiene efecto</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#hasSymptom -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#hasSymptom">
        <owl:inverseOf rdf:resource="https://w3id.org/def/DRUGS4COVID19#isSymptomOfDisease"/>
        <rdfs:comment xml:lang="en">A relation between a disease and one of its symptoms</rdfs:comment>
        <rdfs:label xml:lang="en">Has symptom</rdfs:label>
        <rdfs:label xml:lang="es">Tiene síntoma</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#isActiveSubstanceOfDrug -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#isActiveSubstanceOfDrug">
        <rdfs:comment xml:lang="en">A relation between an active substance and a drug it is an ingredient of</rdfs:comment>
        <rdfs:label xml:lang="es">Es principio activo de medicamento</rdfs:label>
        <rdfs:label xml:lang="en">Is active substance of drug</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#isBroaderThan -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#isBroaderThan">
        <owl:inverseOf rdf:resource="https://w3id.org/def/DRUGS4COVID19#isNarrowerThan"/>
        <rdfs:comment xml:lang="en">A relation between two things in which one is in some way more general  than the other</rdfs:comment>
        <rdfs:label xml:lang="en">Is broader than</rdfs:label>
        <rdfs:label xml:lang="es">es más amplio</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#isContraindicatedTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#isContraindicatedTo">
        <rdfs:comment xml:lang="en">A relation between a drug and one of the diseases it is advised against</rdfs:comment>
        <rdfs:label xml:lang="es">Está contraindicado para</rdfs:label>
        <rdfs:label xml:lang="en">Is contraindicated to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#isNarrowerThan -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#isNarrowerThan">
        <rdfs:comment xml:lang="en">A relation between two things in which one is in some way more specific than the other</rdfs:comment>
        <rdfs:label xml:lang="es">Es más específico</rdfs:label>
        <rdfs:label xml:lang="en">Is narrower than</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#isSymptomOfDisease -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#isSymptomOfDisease">
        <rdfs:comment xml:lang="en">A relation between a symptom and one of the disorders that includes it</rdfs:comment>
        <rdfs:label xml:lang="es">Es síntoma de enfermedad</rdfs:label>
        <rdfs:label xml:lang="en">Is symptom of disease</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#isTreatedWithDrug -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#isTreatedWithDrug">
        <owl:inverseOf rdf:resource="https://w3id.org/def/DRUGS4COVID19#treatsDisorder"/>
        <rdfs:comment xml:lang="en">A relation between a disease and one of the drugs that can be used to deal with it</rdfs:comment>
        <rdfs:label xml:lang="es">Es tratado con medicamento</rdfs:label>
        <rdfs:label xml:lang="en">Is treated with drug</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#mapsTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#mapsTo">
        <rdfs:comment xml:lang="en">A relation between a disease and one of the diseases it is in some way related to</rdfs:comment>
        <rdfs:label xml:lang="en">Maps to</rdfs:label>
        <rdfs:label xml:lang="es">Relacionada con</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#mentions -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#mentions">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/def/DRUGS4COVID19#Paper"/>
                    <rdf:Description rdf:about="https://w3id.org/def/DRUGS4COVID19#Sentence"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/def/DRUGS4COVID19#ChemicalSubstance"/>
                    <rdf:Description rdf:about="https://w3id.org/def/DRUGS4COVID19#Disease"/>
                    <rdf:Description rdf:about="https://w3id.org/def/DRUGS4COVID19#Drug"/>
                    <rdf:Description rdf:about="https://w3id.org/def/DRUGS4COVID19#Symptom"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A relation between a text (a paper, a paragraph or a sentece) and one of the entities it speaks of</rdfs:comment>
        <rdfs:label xml:lang="es">Menciona</rdfs:label>
        <rdfs:label xml:lang="en">Mentions</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#prevents -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#prevents">
        <rdfs:comment xml:lang="en">A relation between a drug and one of the diseases it keeps from occurring</rdfs:comment>
        <rdfs:label xml:lang="en">Prevents</rdfs:label>
        <rdfs:label xml:lang="es">Previene</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#trataEnfermedad -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#trataEnfermedad">
        <rdfs:label xml:lang="es">trata enfermedad</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/DRUGS4COVID19#treatsDisorder -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/DRUGS4COVID19#treatsDisorder">
        <rdfs:comment xml:lang="en">A relation between a drug and one of the diseases it deals with</rdfs:comment>
        <rdfs:label xml:lang="es">Trata desorden</rdfs:label>
        <rdfs:label xml:lang="en">Treats disorder</rdfs:label>
    </owl:ObjectProperty>
    


