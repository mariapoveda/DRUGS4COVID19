        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/DRUGS4COVID19#hasEffect"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/def/DRUGS4COVID19#Effect"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/DRUGS4COVID19#isContraindicatedTo"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/def/DRUGS4COVID19#Disorder"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/DRUGS4COVID19#treatsDisorder"/>
